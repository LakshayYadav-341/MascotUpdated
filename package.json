{
  "name": "servermascot",
  "private": true,
  "version": "0.1.0",
  "description": "Backend for PSLV-REACT-JSX project",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --ext ts --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc -b tsconfig.node.json",
    "start": "ts-node -r tsconfig-paths/register build/server.js",
    "lint": "eslint . --ext .ts --fix"
  },
  "_moduleAliases": {
    "@handlers": "build/handlers",
    "@server": "build/server",
    "@models": "build/models",
    "@types_/mongo": "build/types/mongo",
    "@types_/institute": "build/types/institute"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^7.4.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "request-ip": "^3.3.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "validator": "^13.11.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/module-alias": "^2.0.4",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.10",
    "@types/node": "^20.17.10",
    "@types/request-ip": "^0.0.41",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/validator": "^13.11.1",
    "eslint": "^8.45.0",
    "nodemon": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "author": "Lakshay Yadav",
  "license": "GPL-3.0"
}
